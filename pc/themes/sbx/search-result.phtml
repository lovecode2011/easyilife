<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    <title>圣宝先官方商城</title>
    <meta name="keywords" content=" " />
    <meta name="description" content=" " />
    <meta name="author" content="www.kwanson.com">
    <meta name="version" content="v.1.0.0">
    <meta http-equiv="Cache-Control" content="no-cache">
    <link rel="shortcut icon" href="images/favicon.ico" type="image/x-icon">
    <link href="{$template_dir}css/style.css" rel="stylesheet" type="text/css" />
    <link href="{$template_dir}css/fonts.css" rel="stylesheet" type="text/css" />
</head>
<body>
{include file="header.phtml"}
<!-- 顶部面包屑导航开始 -->
<div class="breadcrumb container">
    <span>当前位置：</span>
    <a href="search-result-product.html" target="_blank">全部结果</a><span class="gt">&gt;</span>
    <span>圣宝先自营</span>
</div>
<!-- 顶部面包屑导航结束 -->
<!-- 全部产品开始 -->
<div class="search-product product-all container">
    <div class="con">
        <div class="filter">
            <div class="f-line top">
                <div class="f-sort"><a href="javascript:;" class="curr">综合排序</a><a href="javascript:;">销量</a><a href="javascript:;">价格<i class="icon"></i></a><a href="javascript:;">评论数</a></div>
                <div class="f-pager"><span class="fp-text"><b>1</b><em>/</em><i>18</i></span><a class="fp-prev disabled" href="javascript:;">&lt;</a><a class="fp-next"  href="javascript:;">&gt;</a></div>
                <div class="f-result-sum">共<span id="J_resCount" class="num">1062</span>件商品</div>
                <span class="clr"></span>
            </div>
        </div>
        <ul class="p-list">
            {foreach from=$product_list item=p}
            <li>
                <div class="p-img">
                    <a href="product.php?id={$p.id}" target="_blank">
                        <img width="160" height="160" alt="{$p.name}" src="../{$p.img}">
                    </a>
                </div>
                <div class="p-name">
                    <a href="product.php?id={$p.id}" title="{$p.name}" target="">
                        {$p.name}
                    </a>
                </div>
                <div class="p-price"><strong>￥{$p.price}</strong></div>
                <div class="extra">
                    <span class="evaluate">
							<a href="#" target="_blank">已有<span>0</span>人评论</a>
                    </span>
                </div>
                <div class="btns">
                    <a class="btn-buy" target="_blank" href="#">加入购物车</a>
                    <a class="btn-coll" target="_blank" href="#">收藏</a>
                </div>
            </li>
            {/foreach}
        </ul>
        <div style="clear: both;"></div>
        {if count($product_list) gt 10}
        <div class="pages fr">
            <a rel="nofollow" class="prev disabled fl" id="prePage" href="javascript:void(0);"><i class="sign left">&lt;</i>上一页</a>
            <a rel="nofollow" class="cur fl" id="pageFirst" href="#">1</a>
            <a rel="nofollow" class="fl" href="#">2</a>
            <a rel="nofollow" class="fl" href="#">3</a>
            <a rel="nofollow" class="fl" href="#">4</a>
            <span class="searchBg ellipsis fl"></span>
            <a rel="nofollow" class="fl" href="#" id="pageLast">100</a>
            <a rel="nofollow" id="nextPage" class="next fl" title="下一页" href="#">下一页<i class="sign right">&gt;</i></a>
            <div class="fl">
                <span class="fl">向第</span>
                <input class="fl" type="text" id="bottomPage">
                <span class="fl">页</span>
                <a rel="nofollow" class="pageGo fl" href="#" id="pagesubmit">跳转</a>
            </div>
        </div>
        {/if}
        <div style="clear: both;"></div>
    </div>
</div>
<!-- 全部产品结束 -->
<!-- 底部开始 -->
<div class="footer container">
    <div class="footer-inner">
        <div class="fl">
            <h3>联系我们</h3>
            <span> CONTACT US</span>
            <span>&nbsp;</span>
            <span class="copy">copyright&copy;圣宝先国际商务服务有限公司</span>
        </div>
        <div class="fr">
            <span>圣宝先国际商务服务有限公司</span>
            <span>电话：440-123-4567</span>
            <span>邮箱：sbx@qq.com</span>
            <span>地址：成都市青羊区堂东路66号锦绣工场古玩城55314</span>
        </div>
    </div>
</div>
<!-- 底部结束 -->
<script src="{$template_dir}js/jquery-1.11.0.min.js" type="text/javascript"></script>
<script type="text/javascript" src="{$template_dir}js/sbx-pc.js"></script>

</body>
</html>